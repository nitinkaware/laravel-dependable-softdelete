{
    "name": "nitinkaware/laravel-dependable-softdelete",
    "type": "library",
    "description": "A laravel dependable soft delete package",
    "keywords": [
        "laravel",
        "laravel-soft-deletable",
        "soft-delete"
    ],
    "homepage": "https://github.com/nitinkaware/laravel-dependable-softdelete.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Nitin Kaware",
            "email": "nitin.kaware1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "NitinKaware\\DependableSoftDeletable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NitinKaware\\DependableSoftDeletable\\": "tests"
        }
    }
}
