{
    "name": "nitinkaware/laravel-dependable-softdelete",
    "type": "library",
    "description": "A laravel dependable soft delete package",
    "keywords": [
        "laravel",
        "laravel-soft-deletable",
        "soft-delete"
    ],
    "homepage": "https://github.com/nitinkaware/laravel-dependable-softdelete.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Nitin Kaware",
            "email": "nitin.kaware1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "orchestra/database": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "NitinKaware\\DependableSoftDeletable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NitinKaware\\DependableSoftDeletable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
